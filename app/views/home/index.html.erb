<div class="panel panel-default">
  <div class="panel-heading"><h4>Inproceedings</h4>

  </div>
  <table class="table">
    <tbody>
    <% @Inproceedings.each do |i| %>
        <tr>
          <td><%= link_to i.title, i  %></td>
        </tr>
    <% end %>
    <tfoot>
    <th>
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle btn-xs" data-toggle="dropdown">
          Actions <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <li><%= link_to 'New inproceedings', new_inproceedings_path %></li>
          <li><%= link_to 'List all', inproceedings_index_path %></li>
        </ul>
      </div>
    </th>
    </tfoot>
    </tbody>
  </table>
</div>

