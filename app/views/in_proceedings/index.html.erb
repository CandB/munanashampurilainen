<h1>Listing in_proceedings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Booktitle</th>
      <th>Year</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @in_proceedings.each do |in_proceeding| %>
      <tr>
        <td><%= in_proceeding.title %></td>
        <td><%= in_proceeding.booktitle %></td>
        <td><%= in_proceeding.year %></td>
        <td><%= link_to 'Show', in_proceeding %></td>
        <td><%= link_to 'Edit', edit_in_proceeding_path(in_proceeding) %></td>
        <td><%= link_to 'Destroy', in_proceeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

